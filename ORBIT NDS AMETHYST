-- // NOTIFICATION SYSTEM
------------------------------------------------------------
local ScreenGui = UILib:Create("ScreenGui", {
    Name = "OrbitUtilityPro",
    Parent = getGuiParent(),
    ResetOnSpawn = false,
    ZIndexBehavior = Enum.ZIndexBehavior.Sibling
})


local NotifyContainer = UILib:Create("Frame", {
    Parent = ScreenGui,
    Size = UDim2.new(0, 300, 1, 0),
    Position = UDim2.new(1, -320, -0.5, 0),
    BackgroundTransparency = 1
})


local function Notify(title, text, duration)
    PlaySound(SFX.Notification, 0.5)
   
    local card = UILib:Create("Frame", {
        Parent = NotifyContainer,
        Size = UDim2.new(1, 0, 0, 70),
        Position = UDim2.new(1, 20, 1, -100), -- Start off screen
        BackgroundColor3 = THEME.Panel,
        BorderSizePixel = 0
    })
   
    Instance.new("UICorner", card).CornerRadius = UDim.new(0, 8)
    local stroke = Instance.new("UIStroke", card)
    stroke.Color = THEME.Accent
    stroke.Transparency = 0.5
   
    local tLabel = UILib:Create("TextLabel", {
        Parent = card,
        Size = UDim2.new(1, -20, 0, 25),
        Position = UDim2.new(0, 10, 0, 5),
        BackgroundTransparency = 1,
        Text = title,
        TextColor3 = THEME.Accent,
        Font = Enum.Font.GothamBold,
        TextSize = 16,
        TextXAlignment = Enum.TextXAlignment.Left
    })
   
    local cLabel = UILib:Create("TextLabel", {
        Parent = card,
        Size = UDim2.new(1, -20, 0, 35),
        Position = UDim2.new(0, 10, 0, 30),
        BackgroundTransparency = 1,
        Text = text,
        TextColor3 = THEME.Text,
        Font = Enum.Font.Gotham,
        TextSize = 14,
        TextXAlignment = Enum.TextXAlignment.Left,
        TextWrapped = true
    })


    -- Animation
    TweenService:Create(card, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {Position = UDim2.new(0, 0, 1, -(#NotifyContainer:GetChildren() * 80))}):Play()
   
    task.delay(duration or 3, function()
        TweenService:Create(card, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {BackgroundTransparency = 1, Position = UDim2.new(1, 50, card.Position.Y.Scale, card.Position.Y.Offset)}):Play()
        TweenService:Create(tLabel, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
        TweenService:Create(cLabel, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
        stroke:Destroy()
        task.wait(0.5)
        card:Destroy()
    end)
end

    

-- // MAIN GUI
local MainFrame = UILib:Create("Frame", {
    Name = "MainFrame",
    Parent = ScreenGui,
    Size = UDim2.new(0, 260, 0, 480),
    Position = UDim2.new(0.05, 0, 0.5, -240),
    BackgroundColor3 = THEME.Background,
    BorderSizePixel = 0,
    ClipsDescendants = true
})
UILib:AddCorner(MainFrame, 12)

local MainStroke = Instance.new("UIStroke", MainFrame)
MainStroke.Color = THEME.Accent
MainStroke.Transparency = 0.6
MainStroke.Thickness = 2
UILib:AddBreathing(MainStroke)

-- Title Bar
local TitleBar = UILib:Create("Frame", {
    Parent = MainFrame,
    Size = UDim2.new(1, 0, 0, 45),
    BackgroundColor3 = THEME.Panel,
    BorderSizePixel = 0
})
UILib:AddCorner(TitleBar, 12) -- Matches top corners
UILib:Gradient(TitleBar)

local TitleText = UILib:Create("TextLabel", {
    Parent = TitleBar,
    Size = UDim2.new(1, 50, 1, 0),
    Position = UDim2.new(0, 15, 0, 0),
    Text = "ORBIT <b>PRO</b>",
    TextColor3 = Color3.new(1,1,1),
    RichText = true,
    Font = Enum.Font.GothamBlack,
    TextSize = 20,
    BackgroundTransparency = 1,
    TextXAlignment = Enum.TextXAlignment.Left
})

-- Dragging
local dragToggle, dragStart, startPos
TitleBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragToggle = true
        dragStart = input.Position
        startPos = MainFrame.Position
    end
end)
TitleBar.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then dragToggle = false end
end)
-- Function to create the amethyst blurry trail
local function createTrail()
    local trail = Instance.new("Frame")
    trail.Name = "Trail"
    trail.Parent = ScreenGui
    trail.Size = MainFrame.Size
    trail.Position = MainFrame.Position
    trail.BackgroundColor3 = Color3.fromRGB(140, 60, 255) -- Amethyst
    trail.BorderSizePixel = 0
    trail.ZIndex = MainFrame.ZIndex - 1
    trail.BackgroundTransparency = 0.6
    
    local corner = Instance.new("UICorner", trail)
    corner.CornerRadius = UDim.new(0, 12)

    -- Blur/Fade Animation
    TweenService:Create(trail, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        Size = MainFrame.Size + UDim2.new(0, 20, 0, 20),
        Position = MainFrame.Position - UDim2.new(0, 10, 0, 10),
        BackgroundTransparency = 1
    }):Play()

    game:GetService("Debris"):AddItem(trail, 0.4)
end

-- Updated Dragging Logic with Trail Trigger
UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement and dragToggle then
        local delta = input.Position - dragStart
        MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        createTrail() -- Triggers the amethyst trail
    end
end)

-- Content Container
local Content = UILib:Create("Frame", {
    Parent = MainFrame,
    Size = UDim2.new(1, -20, 1, -85), -- Adjusted for hint at bottom
    Position = UDim2.new(0, 10, 0, 50),
    BackgroundTransparency = 1
})
local UIList = Instance.new("UIListLayout", Content)
UIList.SortOrder = Enum.SortOrder.LayoutOrder
UIList.Padding = UDim.new(0, 10)

-- // WIDGET BUILDER
local function CreateButton(text, callback)
    local btn = UILib:Create("TextButton", {
        Parent = Content,
        Size = UDim2.new(1, 0, 0, 45),
        BackgroundColor3 = THEME.Panel,
        Text = "",
        AutoButtonColor = false,
        BorderSizePixel = 0
    })
    UILib:AddCorner(btn, 8)
    
    local s = Instance.new("UIStroke", btn)
    s.Color = THEME.AccentDark
    s.Transparency = 0.5
    s.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    UILib:AddBreathing(s)

    local lbl = UILib:Create("TextLabel", {
        Parent = btn,
        Size = UDim2.new(1, 0, 1, 0),
        BackgroundTransparency = 1,
        Text = text,
        TextColor3 = THEME.Text,
        Font = Enum.Font.GothamBold,
        TextSize = 14
    })

    btn.MouseEnter:Connect(function()
        PlaySound(SFX.Hover, 0.3)
        TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = THEME.AccentDark}):Play()
    end)
    btn.MouseLeave:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = THEME.Panel}):Play()
    end)

    btn.MouseButton1Click:Connect(function()
        PlaySound(SFX.Click, 1)
        UILib:Ripple(btn)
        Notify("Action", "Triggered: " .. text, 1) -- Notification on click
        callback(btn, lbl)
    end)

    return btn, lbl
end

-- // FEATURES UI

-- 1. Orbit Toggle
local OrbitBtn, OrbitLbl = CreateButton("ENABLE ORBIT", function(btn, lbl)
    State.Orbiting = not State.Orbiting
    -- Update is handled by the Heartbeat check below to ensure sync
end)
OrbitButtonRef = OrbitLbl -- Save reference

-- Reactive UI Loop for Orbit Button
task.spawn(function()
    while true do
        if OrbitButtonRef then
            if State.Orbiting then
                OrbitButtonRef.Text = "DISABLE ORBIT"
                OrbitButtonRef.TextColor3 = THEME.Success
            else
                OrbitButtonRef.Text = "ENABLE ORBIT"
                OrbitButtonRef.TextColor3 = THEME.Text
                -- Cleanup when disabled
                if #ActiveParts > 0 then
                    for _, p in ipairs(ActiveParts) do
                        if p:FindFirstChild("ProOrbitBP") then p.ProOrbitBP:Destroy() end
                        if p:FindFirstChild("ProOrbitBG") then p.ProOrbitBG:Destroy() end
                        p.CanCollide = true
                        p.Transparency = 0
                    end
                    table.clear(ActiveParts)
                    Notify("System", "Orbit Disabled - Parts Released", 2)
                end
            end
        end
        task.wait(0.2)
    end
end)

-- 2. Axis Switcher
CreateButton("AXIS: HORIZONTAL", function(btn, lbl)
    local idx = table.find(Modes, State.Axis) or 1
    idx = (idx % #Modes) + 1
    State.Axis = Modes[idx]
    lbl.Text = "AXIS: " .. State.Axis:upper()
end)

-- 3. Anti-Fall
CreateButton("ANTI-FALL: OFF", function(btn, lbl)
    State.AntiFall = not State.AntiFall
    if State.AntiFall then
        if game.PlaceId == 189707 then -- Natural Disaster Survival ID
            local function protect(char)
                local r = char:WaitForChild("HumanoidRootPart", 5)
                if not r then return end
                Connections.AntiFall = RunService.Heartbeat:Connect(function()
                    if not State.AntiFall then return end
                    local oldVel = r.AssemblyLinearVelocity
                    if r.Position.Y < -40 then -- Basic height check
                        r.AssemblyLinearVelocity = Vector3.zero
                        RunService.RenderStepped:Wait()
                        r.AssemblyLinearVelocity = oldVel
                        r.Position = r.Position + Vector3.new(0, 100, 0)
                    end
                end)
            end
            if Player.Character then protect(Player.Character) end
            Connections.CharAdd = Player.CharacterAdded:Connect(protect)
            lbl.Text = "ANTI-FALL: ON"
            lbl.TextColor3 = THEME.Success
        else
            State.AntiFall = false
            Notify("Error", "Anti-Fall optimized for NDS", 3)
        end
    else
        if Connections.AntiFall then Connections.AntiFall:Disconnect() end
        if Connections.CharAdd then Connections.CharAdd:Disconnect() end
        lbl.Text = "ANTI-FALL: OFF"
        lbl.TextColor3 = THEME.Text
    end
end)

-- 4. Kamehameha
local FireBtn, FireLbl = CreateButton("CHARGE ENERGY", function(btn, lbl)
    if #ActiveParts == 0 then
        Notify("Warning", "No parts collected to throw!", 2)
        return
    end

    State.Charging = true
    PlaySound(SFX.PowerUp, 1)
    Notify("Kamehameha", "Charging... Click anywhere to fire!", 4)
    
    -- Visual Pulse
    TweenService:Create(MainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {BackgroundColor3 = THEME.AccentDark}):Play()
end)

UserInputService.InputBegan:Connect(function(input, gpe)
    if gpe then return end
    if input.UserInputType == Enum.UserInputType.MouseButton1 and State.Charging then
        State.Charging = false
        State.Orbiting = false -- Stop orbiting to release
        
        -- Play Release Sound
        PlaySound(SFX.Release, 1)

        -- Reset UI Pulse
        TweenService:Create(MainFrame, TweenInfo.new(0.5), {BackgroundColor3 = THEME.Background}):Play()
        Notify("Kamehameha", "RELEASED!", 2)
        
        -- Physics Throw
        local target = Mouse.Hit.Position
        for _, part in ipairs(ActiveParts) do
            if part and part.Parent then
                if part:FindFirstChild("ProOrbitBP") then part.ProOrbitBP:Destroy() end
                if part:FindFirstChild("ProOrbitBG") then part.ProOrbitBG:Destroy() end
                
                local bv = Instance.new("BodyVelocity")
                bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
                bv.Velocity = (target - part.Position).Unit * SETTINGS.ThrowVelocity
                bv.Parent = part
                Debris:AddItem(bv, 3)
                
                part.Transparency = 0
                part.CanCollide = true
            end
        end
        table.clear(ActiveParts)
    end
end)

-- // SLIDERS (PRO FEATURE)
local function CreateSlider(text, min, max, default, callback)
    local frame = UILib:Create("Frame", {
        Parent = Content,
        Size = UDim2.new(1, 0, 0, 40),
        BackgroundTransparency = 1
    })
    
    local title = UILib:Create("TextLabel", {
        Parent = frame,
        Size = UDim2.new(1, 0, 0, 20),
        BackgroundTransparency = 1,
        Text = text .. ": " .. default,
        TextColor3 = THEME.TextDim,
        Font = Enum.Font.Gotham,
        TextSize = 12,
        TextXAlignment = Enum.TextXAlignment.Left
    })
    
    local bg = UILib:Create("TextButton", {
        Parent = frame,
        Size = UDim2.new(1, 0, 0, 6),
        Position = UDim2.new(0, 0, 0, 25),
        BackgroundColor3 = THEME.Panel,
        AutoButtonColor = false,
        Text = ""
    })
    UILib:AddCorner(bg, 100)
    
    local fill = UILib:Create("Frame", {
        Parent = bg,
        Size = UDim2.new((default - min)/(max - min), 0, 1, 0),
        BackgroundColor3 = THEME.Accent
    })
    UILib:AddCorner(fill, 100)
    
    local dragging = false
    
    local function update(input)
        local pos = math.clamp((input.Position.X - bg.AbsolutePosition.X) / bg.AbsoluteSize.X, 0, 1)
        local val = math.floor(min + ((max - min) * pos))
        
        fill.Size = UDim2.new(pos, 0, 1, 0)
        title.Text = text .. ": " .. val
        callback(val)
    end
    
    bg.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            update(input)
        end
    end)
    
    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            update(input)
        end
    end)
end

CreateSlider("SPEED", 5, 500, SETTINGS.OrbitSpeed, function(v) -- Max updated to 500
    SETTINGS.OrbitSpeed = v 
end)
CreateSlider("RADIUS", 5, 500, SETTINGS.OrbitRadius, function(v) -- Max updated to 500
    SETTINGS.OrbitRadius = v 
end)

-- // HINT & TOGGLE
local Hint = UILib:Create("TextLabel", {
    Parent = MainFrame,
    Size = UDim2.new(1, 0, 0, 20),
    Position = UDim2.new(0, 0, 1, -25),
    BackgroundTransparency = 1,
    Text = "Press [L-CTRL] to Toggle UI",
    TextColor3 = THEME.TextDim,
    Font = Enum.Font.Gotham,
    TextSize = 10
})

local toggled = true
UserInputService.InputBegan:Connect(function(input, gpe)
    if not gpe and input.KeyCode == Enum.KeyCode.LeftControl then
        toggled = not toggled
        PlaySound(SFX.Click, 1)
        if toggled then
            MainFrame.Visible = true
            MainFrame.Size = UDim2.new(0, 260, 0, 0)
            TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back), {Size = UDim2.new(0, 260, 0, 480)}):Play()
        else
            TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {Size = UDim2.new(0, 260, 0, 0)}):Play()
            task.wait(0.3)
            if not toggled then MainFrame.Visible = false end
        end
    end
end)

