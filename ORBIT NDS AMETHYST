-- // SERVICES
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local SoundService = game:GetService("SoundService")
local Debris = game:GetService("Debris")
local CoreGui = game:GetService("CoreGui")

-- // CONFIGURATION & THEME
local THEME = {
    Background = Color3.fromRGB(10, 10, 15),
    Panel = Color3.fromRGB(18, 18, 26),
    Accent = Color3.fromRGB(140, 60, 255), -- Amethyst
    AccentDark = Color3.fromRGB(90, 30, 180),
    Gradient1 = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(170, 80, 255)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(100, 40, 200))
    }),
    Text = Color3.fromRGB(240, 240, 255),
    TextDim = Color3.fromRGB(160, 160, 180),
    Error = Color3.fromRGB(255, 60, 60),
    Success = Color3.fromRGB(140, 60, 255) -- Changed Green to Amethyst
}

local SETTINGS = {
    OrbitSpeed = 15,
    OrbitRadius = 20,
    HeightOffset = 3,
    ScanRate = 1.0, -- Lower is faster scanning
    ThrowVelocity = 600
}

-- // PLAYER & UTILS
local Player = Players.LocalPlayer
local Mouse = Player:GetMouse()
local Camera = workspace.CurrentCamera

-- Safe GUI Parent (Exploit Protection)
local function getGuiParent()
    return (CoreGui:FindFirstChild("RobloxGui") or Player:WaitForChild("PlayerGui"))
end

-- Cleanup Old Instances
for _, v in ipairs(getGuiParent():GetChildren()) do
    if v.Name == "OrbitUtilityPro" then v:Destroy() end
end

-- // SOUND ENGINE
local SFX = {
    Hover = "rbxassetid://6895079853",
    Click = "rbxassetid://435186630",
    Notification = "rbxassetid://4590657391",
    PowerUp = "rbxassetid://2696683313",
    Release = "rbxassetid://132944341377293" -- [User Note: This ID is very long, ensure it is correct]
}

local function PlaySound(id, vol)
    task.spawn(function()
        local s = Instance.new("Sound")
        s.SoundId = id
        s.Volume = vol or 1
        s.Parent = SoundService
        s.PlayOnRemove = true
        s:Play()
        s:Destroy()
    end)
end

-- // UI LIBRARY (MINIFIED & ENHANCED)
local UILib = {}

function UILib:AddCorner(instance, radius)
    local corner = Instance.new("UICorner", instance)
    corner.CornerRadius = UDim.new(0, radius or 8)
    return corner
end

function UILib:AddBreathing(stroke)
    task.spawn(function()
        while stroke and stroke.Parent do
            local t1 = TweenService:Create(stroke, TweenInfo.new(1.5, Enum.EasingStyle.Sine), {Transparency = 0.2})
            t1:Play()
            t1.Completed:Wait()
            local t2 = TweenService:Create(stroke, TweenInfo.new(1.5, Enum.EasingStyle.Sine), {Transparency = 0.7})
            t2:Play()
            t2.Completed:Wait()
        end
    end)
end

function UILib:Create(class, props)
    local obj = Instance.new(class)
    for k, v in pairs(props) do
        obj[k] = v
    end
    -- Auto Corner & Stroke for specific types could go here, but doing manually for precision
    return obj
end

function UILib:Gradient(parent)
    local g = Instance.new("UIGradient")
    g.Color = THEME.Gradient1
    g.Rotation = 45
    g.Parent = parent
    return g
end

function UILib:Ripple(button)
    local circle = Instance.new("ImageLabel")
    circle.BackgroundTransparency = 1
    circle.Image = "rbxassetid://266543268"
    circle.ImageTransparency = 0.6
    circle.ImageColor3 = Color3.new(1,1,1)
    circle.ZIndex = 10
    circle.Parent = button
    local x, y = Mouse.X - button.AbsolutePosition.X, Mouse.Y - button.AbsolutePosition.Y
    circle.Position = UDim2.new(0, x, 0, y)
    circle.Size = UDim2.new(0, 0, 0, 0)
    TweenService:Create(circle, TweenInfo.new(0.4), {Size = UDim2.new(0, 300, 0, 300), ImageTransparency = 1}):Play()
    Debris:AddItem(circle, 0.45)
end

-- // STATE VARIABLES
local State = {
    Orbiting = false,
    AntiFall = false,
    Noclip = false,
    Charging = false,
    Axis = "Horizontal"
}
local Modes = {"Horizontal", "Vertical", "Cross", "Sphere"}
local ActiveParts = {}
local Connections = {}
local OrbitButtonRef = nil -- Reference for auto-updating button

-- // PHYSICS ENGINE
local function IsValidPart(part)
    return part:IsA("BasePart") 
        and not part.Anchored 
        and not part:IsDescendantOf(Player.Character) 
        and part.Mass < 5000 
end

--[[
	WARNING: Heads up! This script has not been verified by ScriptBlox. Use at your own risk!
]]
local Noclip = nil
local Clip = nil

function noclip()
	Clip = false
	local function Nocl()
		if Clip == false and game.Players.LocalPlayer.Character ~= nil then
			for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
				if v:IsA('BasePart') and v.CanCollide and v.Name ~= floatName then
					v.CanCollide = false
				end
			end
		end
		wait(0.21) -- basic optimization
	end
	Noclip = game:GetService('RunService').Stepped:Connect(Nocl)
end

function clip()
	if Noclip then Noclip:Disconnect() end
	Clip = true
end

noclip() -- to toggle noclip() and clip()

-- Orbit Logic
RunService.Heartbeat:Connect(function()
    if not State.Orbiting then return end
    
    local char = Player.Character
    local root = char and char:FindFirstChild("HumanoidRootPart")
    if not root then return end

    -- Cleanup invalid parts
    for i = #ActiveParts, 1, -1 do
        local p = ActiveParts[i]
        if not p or not p.Parent or p.Anchored then
            table.remove(ActiveParts, i)
        end
    end

    local count = #ActiveParts
    if count == 0 then return end

    local t = tick() * SETTINGS.OrbitSpeed
    
    for i, part in ipairs(ActiveParts) do
        local bp = part:FindFirstChild("ProOrbitBP") or Instance.new("BodyPosition", part)
        bp.Name = "ProOrbitBP"
        bp.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        bp.P = 25000
        bp.D = 800

        local bg = part:FindFirstChild("ProOrbitBG") or Instance.new("BodyGyro", part)
        bg.Name = "ProOrbitBG"
        bg.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
        bg.P = 3000

        local angle = t + (i * (math.pi * 2 / count))
        
        -- Advanced Axis Math (Relative to Character Rotation)
        local relativeOffset = Vector3.zero
        
        if State.Axis == "Horizontal" then
            relativeOffset = Vector3.new(math.cos(angle) * SETTINGS.OrbitRadius, 0, math.sin(angle) * SETTINGS.OrbitRadius)
        elseif State.Axis == "Vertical" then
            relativeOffset = Vector3.new(0, math.cos(angle) * SETTINGS.OrbitRadius, math.sin(angle) * SETTINGS.OrbitRadius)
        elseif State.Axis == "Cross" then
            if i % 2 == 0 then
                relativeOffset = Vector3.new(math.cos(angle) * SETTINGS.OrbitRadius, 0, math.sin(angle) * SETTINGS.OrbitRadius)
            else
                relativeOffset = Vector3.new(0, math.cos(angle) * SETTINGS.OrbitRadius, math.sin(angle) * SETTINGS.OrbitRadius)
            end
        elseif State.Axis == "Sphere" then
            local phi = math.acos(1 - 2 * (i / count))
            local theta = math.sqrt(count * math.pi) * phi + t
            relativeOffset = Vector3.new(math.cos(theta) * math.sin(phi), math.sin(theta) * math.sin(phi), math.cos(phi)) * SETTINGS.OrbitRadius
        end

        -- Calculate World Position based on Character CFrame
        local worldOffset = root.CFrame:VectorToWorldSpace(relativeOffset)
        
        bp.Position = root.Position + Vector3.new(0, SETTINGS.HeightOffset, 0) + worldOffset
        bg.CFrame = CFrame.lookAt(part.Position, root.Position)

        -- Visual effect
        part.CanCollide = false
        part.Transparency = 0.2
    end
end)

-- Background Scanner
task.spawn(function()
    while true do
        if State.Orbiting then
            for _, v in ipairs(workspace:GetDescendants()) do
                if IsValidPart(v) and not table.find(ActiveParts, v) then
                    table.insert(ActiveParts, v)
                    -- Unstick
                    v.Velocity = Vector3.new(0, 10, 0)
                end
            end
        end
        task.wait(SETTINGS.ScanRate)
    end
end)

